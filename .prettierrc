{
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "overrides": [
            {
                    "files": "*.vimrc",
                    "options": {
                            "tabWidth": 4
                    }
            }
    ],
    "trailingComma": "es5",
    "useTabs": false,
    "proseWrap": "never"
}
